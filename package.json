{
  "name": "expo-masonry-layout",
  "version": "1.1.2",
  "description": "High-performance masonry layout component for React Native and Expo applications",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "react-native": "./lib/index.js",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.js",
      "react-native": "./lib/index.js",
      "default": "./lib/index.js"
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf lib",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\""
  },
  "files": [
    "lib",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "react-native",
    "expo",
    "masonry",
    "layout",
    "grid",
    "virtualized",
    "performance"
  ],
  "author": "Echowaves Corp.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/echowaves/expo-masonry-layout.git"
  },
  "bugs": {
    "url": "https://github.com/echowaves/expo-masonry-layout/issues"
  },
  "homepage": "https://github.com/echowaves/expo-masonry-layout#readme",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native": ">=0.70.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/eslint-parser": "^7.23.0",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/eslintrc": "^3.0.0",
    "@react-native-community/eslint-config": "3.2.0",
    "@types/react": "18.2.0",
    "@types/react-native": "0.72.0",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "eslint": "8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-react-native": "4.1.0",
    "jest": "29.7.0",
    "prettier": "3.4.2",
    "typescript": "5.7.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
